{
  "name": "fandanzle-v2",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "cleanup": "rimraf dist/",
    "test": "jest",
    "prestart": "rimraf dist",
    "prebuild": "rimraf dist",
    "devserver": "cross-env NODE_ENV=development TARGET=client webpack serve --config webpack.config.js --mode development --inline --hot",
    "prodserver": "cross-env NODE_ENV=production TARGET=client webpack serve --config webpack.config.js --mode production",
    "dev": "yarn cleanup && yarn build:style-typings && NODE_ENV=development IS_LOCAL=TRUE NODE_RUNTIME_ENV=development-local NODE_ENV=development concurrently -r --kill-others \"yarn devserver\" \"webpack --config ./webpack/serverless/index.js --watch\" \"sls offline start --noPrependStageInUrl --stage=local-development\"",
    "dev:pure": "yarn cleanup && yarn build:style-typings && NODE_ENV=development IS_LOCAL=TRUE NODE_RUNTIME_ENV=local NODE_ENV=development concurrently -r --kill-others \"yarn devserver\" \"webpack --config ./webpack/server/index.js --watch\" \"wait-on ./dist/server.js && node dist/server.js\"",
    "prod": "yarn cleanup && yarn build:style-typings && yarn build:prod && NODE_ENV=production IS_LOCAL=TRUE NODE_RUNTIME_ENV=production-local concurrently -r --kill-others \"yarn prodserver\" \"sls offline start --noPrependStageInUrl --stage=production\"",
    "build:client:dev": "yarn cleanup && cross-env NODE_ENV=development TARGET=client webpack",
    "build:client:prod": "yarn cleanup && cross-env NODE_ENV=production TARGET=client webpack",
    "build:dev": "yarn cleanup && cross-env NODE_ENV=development yarn build:client:dev && TARGET=serverless webpack",
    "build:prod": "yarn cleanup && cross-env NODE_ENV=production yarn build:client:prod && TARGET=serverless webpack",
    "build:dev:pure": "yarn cleanup && cross-env NODE_ENV=development yarn build:client:dev && TARGET=server webpack",
    "build:prod:pure": "yarn cleanup && cross-env NODE_ENV=production yarn build:client:prod && TARGET=server webpack",
    "deploy:dev": "yarn build:dev && serverless deploy -v --stage development",
    "deploy:staging": "yarn build:prod && serverless deploy -v --stage staging",
    "deploy:prod": "yarn build:prod && serverless deploy -v --stage production",
    "test:types": "tsc --noEmit",
    "hook:before:offline:start": "yarn build:client:prod",
    "build:style-typings": "node webpack/style-typings.js",
    "lint": "eslint ./src --ext .js --ext .ts --ext .tsx",
    "lint:fix": "eslint ./src --ext .js --ext .ts --ext .tsx --fix",
    "types": "graphql-codegen --config codegen.yml"
  },
